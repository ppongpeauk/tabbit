{
  "name": "tabbit-server",
  "version": "1.0.0",
  "description": "Tabbit API server built with ElysiaJS",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir ./dist --target bun",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/s3-presigned-post": "^3.943.0",
    "@aws-sdk/s3-request-presigner": "^3.943.0",
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/swagger": "^1.1.1",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "bcryptjs": "^3.0.3",
    "better-auth": "^1.4.5",
    "elysia": "^1.1.0",
    "ioredis": "^5.8.2",
    "openai": "^4.28.0",
    "pg": "^8.16.3",
    "resend": "^6.5.2",
    "sharp": "^0.34.5",
    "zxing-wasm": "^2.2.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/bun": "latest",
    "@types/pg": "^8.15.6",
    "dotenv": "^17.2.3",
    "prisma": "^7.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
